<robot name="test_robot">
  <link name="base_link" />
  <link name="link1" />
  <link name="link2" />
  <link name="link3" />
  <link name="link4" />
  <link name="link5" />
  <link name="end_link" />

  <joint name="translation_x" type="prismatic">
    <limit upper="100" lower="-100" effort="10" velocity="10" />
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="1 0 0" />
  </joint>  

  <joint name="translation_y" type="prismatic">
    <limit upper="100" lower="-100" effort="10" velocity="10" />
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <joint name="translation_z" type="prismatic">
    <limit upper="100" lower="-100" effort="10" velocity="10" />
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
  </joint>

    <joint name="rotation_x" type="continuous">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="1 0 0" />
  </joint>

  <joint name="rotation_y" type="continuous">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>


  <joint name="rotation_z" type="continuous">
    <parent link="link5"/>
    <child link="end_link"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
  </joint>
</robot>